	<script type="text/javascript">
		
		function openPopup() {
			var newwin = window.open("notice.html", "", "width=500, height=500");
			if(newwin == null){
				alert('팝업이 차단되어 있습니다. 팝업 차단을 해제해 주세요.');
			}
			
			newwin.moveBy(100, 100);
		}
		
		openPopup();
	</script>
