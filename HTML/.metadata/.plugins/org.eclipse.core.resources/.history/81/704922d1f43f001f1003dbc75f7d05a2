<body>
	<script>
		// prompt로 두 개의 정수를 입력받아서 합을 화면에 출력하는 화살표 함수
		var num1 = parseInt(prompt("첫 번째 정수 입력"));
		var num2 = parseInt(prompt("두 번째 정수 입력"));
		const sum = (num1, num2) => document.write(num1+num2);
		sum(num1, num2);
	</script>
</body>
